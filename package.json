        "web-vitals": "^2.1.4",
        "yup": "^1.2.0"
    },
    "scripts": {
        "start": "craco start",
        "setup-env": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/",
        "dev": "npm run setup-env && craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "react-scripts eject",
        "lint": "eslint src --color",
        "prettier": "prettier src -check",
        "lint-staged": "lint-staged --config lint-staged.js",
        "husky-install": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!antd|@ant-design|rc-.+?|@babel/runtime).+(js|jsx)$"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/styled-components": "^5.1.25",
        "eslint": "^8.26.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "tsc-files": "^1.1.3"
    }
