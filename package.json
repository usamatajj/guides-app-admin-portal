        "@hookform/resolvers": "^3.1.1",
        "@tanstack/react-query": "^4.29.25",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.47",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "antd": "^4.22.3",
        "craco-less": "^3.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.1",
        "react-otp-input": "^3.0.2",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.0",
